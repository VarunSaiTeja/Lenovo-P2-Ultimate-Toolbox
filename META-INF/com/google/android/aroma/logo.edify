    if
      confirm(
        #-- Title
          "Confirm Bear Boot Logo",
        #-- Text
          "Please Select Yes or No?",
        #-- Icon (Optional)
          "@logo"
      )=="yes"
    then
	  include("install.edify");
  else
  goto("logo");
  endif;
 endif;
	
