  #-------------------------------+------------------------------------------------+------------------+---------------------#
  
    "Bear","","@Bear",
	"Captain America","","@CapAmerica",
	"Captain America 1","","@CapAmerica1",
	"Captain America 2","","@CapAmerica2",
	"Compass","","@Compass",
	"Dead Pool","","@DPool",
	"Eagle","","@Eagle",
	"G Logo","","@G",
